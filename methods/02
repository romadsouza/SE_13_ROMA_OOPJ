class Restaurant {
    // Instance variables
    private String restaurantName;
    private String address;
    private String contactNumber;
    
    // Constructor to initialize the instance variables
    public Restaurant(String restaurantName, String address, String contactNumber) {
        this.restaurantName = restaurantName;
        this.address = address;
        this.contactNumber = contactNumber;
    }
    
    // Method to display the details of the restaurant
    public void displayRestaurantDetails() {
        System.out.println("Restaurant Name: " + restaurantName);
        System.out.println("Address: " + address);
        System.out.println("Contact Number: " + contactNumber);
    }
}

class Tester {
    public static void main(String[] args) {
        // Create an object of the Restaurant class and initialize instance variables
        Restaurant restaurant = new Restaurant("The Gourmet Kitchen", "123 Main St, Cityville", "123-456-7890");
        
        // Invoke the displayRestaurantDetails() method
        restaurant.displayRestaurantDetails();
    }
}
